<div class="wr-sharebar wr-sharebar-desktop hidden-xs hidden-print noprint"{{#id}} id="sharebar-{{.}}"{{/id}}>
	<ul>
		{{#services}}
				<li class="wr-sharebar-{{name}}">
					{{#arbitraryhtml?}}{{{html}}}{{/arbitraryhtml?}}{{^arbitraryhtml?}}
					<a href="{{url}}" class="wr-share-link{{class}}" data-service="{{name}}" data-action="{{action}}" data-tooltip="{{screenReaderText}}"  data-position="right center" rel="nofollow" target="_blank">
						{{#screenReaderText}}
							<span class="sr-only">{{.}}</span>
						{{/screenReaderText}}
					</a>{{/arbitraryhtml?}}
				</li>
		{{/services}}
	</ul>
</div>
