<div class="wr-sharebar wr-sharebar-desktop hidden-xs hidden-print noprint"{{#id}} id="sharebar-{{.}}"{{/id}}>
	<ul>
		{{#services}}
				<li class="wr-sharebar-{{name}}">
					{{#arbitraryhtml?}}{{{html}}}{{/arbitraryhtml?}}{{^arbitraryhtml?}}
					<a href="{{url}}" class="wr-share-link{{class}}" data-service="{{name}}" data-action="{{action}}"  rel="nofollow" target="_blank">
						<span class="fa fa-{{icon}}"></span>
						{{#text}}<span class="text">{{.}}</span><span class="sr-only">{{.}}</span>{{/text}}
					</a>{{/arbitraryhtml?}}
				</li>
		{{/services}}
	</ul>
</div>
